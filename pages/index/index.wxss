.container {
  height: 100vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
  background-color: #0c5a84;
  color: #ffffff;
}
.detail {
  width: 100%;
  display: flex;
  flex-direction: column;
}
.bar {
  display: flex;
  flex-direction: row;
  margin: 20rpx 0;
}
.box {
  width: 33.33%;
  text-align: center;
}
text {
  font-size: 60rpx;
  color: #b7d6e7;
}
image {
  width: 440rpx;
}